          <area shape="rect" coords="236,22,310,101"    href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Diode 1: 1N5401">
          <area shape="rect" coords="387,22,458,101"    href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Diode 2: 1N5401">
