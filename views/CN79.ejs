          <area shape="rect" coords="478,296,530,380"   href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Capacitor 1: <%=c_Values.c%> micro-farad">
          <area shape="rect" coords="594,298,636,380"   href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Capacitor 2: 2.2 micro-farad">
          <area shape="rect" coords="869,286,903,355"   href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Capacitor 3: 1 micro-farad">
          <area shape="rect" coords="950,286,995,356"   href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Capacitor 4: 10 micro-farad">
          
          <area shape="rect" coords="86,145,169,306"    href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Transformer: <%=c_Values.v_a%> VA">

          <area shape="rect" coords="3,168,60,312"      href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Power Supply: <%=c_Values.v_in> volt">

          <area shape="rect" coords="740,74,780,142"    href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Earthing">

          <area shape="rect" coords="1055,240,1106,355" href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Load: <%=c_Values.vout%> volt">

          <area shape="rect" coords="694,428,831,517"   href="#" data-placement="top"  data-toggle="popover" data-trigger="focus" title="Voltage Regulator : <%=c_Values.img_label%>">
       </map>
