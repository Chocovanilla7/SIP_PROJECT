<!-- Sidebar -->
<div class="w3-sidebar list-group-item list-group-item-light" style="width:25%">

  <div class="list-group-item list-group-item- ">
  <center><h2>USER INPUT</h2></center>
</div>

<form class="w3-container">
   <fieldset disabled>
    <div class="form-group">
    <label for="disabledTextInput">V(RMS)</label>
    <input type="text" id="Input1" class="form-control" value="<%=c_Values.v_in%>" placeholder="Vin(RMS)">
    </div>
    <fieldset disabled>
    <div class="form-group">
      <label for="disabledTextInput">Frequency</label>
      <input type="text" id="Input2" class="form-control" value="<%=c_Values.freq%>"  placeholder="Input">
    </div>
 <fieldset disabled>
    <div class="form-group">
      <label for="disabledTextInput">Ambient Temperature</label>
      <input type="text" id="Output" class="form-control" placeholder="Output">
    </div>
    <div class="form-group">
      <label for="disabledTextInput">VOut</label>
      <input type="text" id="Output" class="form-control" value="<%=c_Values.vout%>"  placeholder="Output">
    </div>
</form><br>

<div class="list-group-item list-group-item- ">
  <center><h2>OUTPUT </h2></center>
</div><br>
<form class="">
   <fieldset disabled>
    <div class="form-group">
      <label for="disabledTextInput">output</label>
      <input type="text" id="Input1" class="form-control" value="<%=c_Values.c%>"  placeholder="Input">
    </div>
    <fieldset disabled>
    <div class="form-group">
      <label for="disabledTextInput">output</label>
      <input type="text" id="Input2" class="form-control" value="<%=c_Values.v_a%>" placeholder="Input">
    </div>
 <fieldset disabled>
    <div class="form-group">
      <label for="disabledTextInput">Output</label>
      <input type="text" id="Output" class="form-control" value="<%=c_Values.img_label%>" placeholder="Output">
    </div>
</form>
</div>

<!-- Page Content -->
<div style="margin-left:25%">

<div class="w3-container">
  <h1></h1>

</div>

 <div class="container border">
<br>
<br>

 <img src="circuita/CN79.png" alt="Workplace" usemap="#workmap" width="1124" height="591">
