  <body>
    <div class="jumbotron text-center" style="margin-bottom:0">
      <h1>CIRCUITATION </h1>
      <h4>A Circuit Design Provider And Simulation Website</h4> 
    </div>
    <div>
      <nav class="navbar navbar-expand-sm aa bg-secondary navbar-dark ">
        <button class="w3-button text-white w3-xlarge" onclick="w3_open()">â˜°</button>
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link text-white" href="#">BACK</a>
          </li>
          <li class="nav-item ">
            <a class="nav-link text-white" href="#">LOGOUT</a>
          </li>
        </ul>
      </nav>
    </div>

    <!-- Sidebar -->
    <div class="w3-sidebar w3-light-grey w3-bar-block w3-border-right" style="display:none" id="mySidebar">
      <button onclick="w3_close()" class="w3-bar-item w3-large">CLOSE &times;</button>
      <div class="list-group-item list-group-item-">
        <center><h2>USER INPUT</h2></center>
      </div>
      <form class="w3-container">
        <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Vin</label>
            <input type="text" id="Vin" class="form-control"  value=" <%=c_Values.v_in%> volt" placeholder="Vin">
          </div>
          <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Frequency</label>
            <input type="text" id="Frequency" class="form-control"  value=" <%=c_Values.freq%> Hz" placeholder="Frequency">
          </div>
          <fieldset disabled>
            <div class="form-group">
              <label for="disabledTextInput">VOut</label>
              <input type="text" id="VOut" class="form-control" value="<%=c_Values.vout%>" placeholder="VOut">
            </div>
      </form><br>

      <div class="w3-container list-group-item list-group-item- ">
        <center><h2>OUTPUT</h2></center>
      </div><br>

      <form class="">
        <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Capacitor (C1)</label>
            <input type="text" id="Capacitor" class="form-control"  value=" <%=c_Values.c%>"  placeholder="Capacitor">
          </div>
          <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Transformer</label>
            <input type="text" id="Transformer" class="form-control"  value=" <%=c_Values.v_a%> VA" placeholder="Transformer">
          </div>
      <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Voltage Regulator</label>
            <input type="text" id="Voltage Regulator" class="form-control" value=" <%=c_Values.img_label%>" placeholder="Voltage Regulator">
          </div>
      </form>
   </div>

    <!-- Page Content -->
    <div class="">

     <div class="w3-container">
        <h1></h1>
     </div>

      <div class="imagebox">
          <br><br>

          <img src="/circuita/CP317.png" alt="Workplace" usemap="#workmap" width="1124" height="591">
        <map name="workmap">
              
          <area shape="rect" coords="304,38,351,108"  href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Diode 1: 1N5401">
          <area shape="rect" coords="408,39,454,109"  href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Diode 2: 1N5401">
          <area shape="rect" coords="304,233,348,305" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Diode 3: 1N5401">
          <area shape="rect" coords="414,235,456,303" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Diode 4: 1N5401">
          <area shape="rect" coords="776,10,821,78"   href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Diode 5: 1N5401">

          <area shape="rect" coords="882,413,942,492" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Resistor 1: <%=c_Values.R2%> ohm">
          <area shape="rect" coords="883,221,941,301" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Resistor 2: 120 ohm" >

          <area shape="rect" coords="611,322,643,400" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Capacitor 1: <%=c_Values.c%> micro-farad">
          <area shape="rect" coords="677,323,712,401" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Capacitor 2: 0.22 micro-farad">
          <area shape="rect" coords="783,408,820,483" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Capacitor 3: 0.1 micro-farad">
          <area shape="rect" coords="961,325,998,400" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Capacitor 4: 10 micro-farad">
            
          <area shape="rect" coords="87,264,165,377"  href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Transformer: <%=c_Values.v_a%> VA">

          <area shape="rect" coords="1,251,57,408"    href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Power Supply: <%=c_Values.v_in%> volt">

          <area shape="rect" coords="581,538,621,582" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Earthing">
            
          <area shape="rect" coords="740,119,865,234" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Voltage Regulator: <%=c_Values.img_label%>">
                
          <area shape="rect" coords="1079,324,1113,398" href="#" data-placement="top" data-toggle="popover" data-trigger="focus" title="Load: <%=c_Values.vout%> volt">
        </map>
      </div>
    </div>
    <script>
      function w3_open()
      {
        document.getElementById("mySidebar").style.width = "25%";
        document.getElementById("mySidebar").style.display = "block";
      }
 
       function w3_close() {
        document.getElementById("mySidebar").style.display = "none";
       }
    </script>
 </body>
</html>