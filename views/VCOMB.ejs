          <area shape="rect" coords="1030,257,1080,360" href="#" data-placement="top"  data-toggle="popover"  data-trigger="focus" title="Load:<%=c_Values.v_psv.vout%> , <%=c_Values.v_neg.vout%> volt">

          
          <area shape="rect" coords="566,65,685,144"    href="#" data-placement="top"  data-toggle="popover"  data-trigger="focus" title="Voltage Regulator: <%=c_Values.v_psv.img_label%>">
          <area shape="rect" coords="565,440,685,525"   href="#" data-placement="top"  data-toggle="popover"  data-trigger="focus" title="Voltage Regulator: <%=c_Values.v_neg.img_label%>">

          <area shape="rect" coords="1060,130,1122,90"  href="#" data-placement="top"  data-toggle="popover"  data-trigger="focus" title="Positive Output">

          <area shape="rect" coords="1065,465,1121,502" href="#" data-placement="top"  data-toggle="popover"  data-trigger="focus" title="Negative Output">
        </map>
      </div>
    </div>
    <script>
      function w3_open()
      {
        document.getElementById("mySidebar").style.width = "25%";
        document.getElementById("mySidebar").style.display = "block";
      }
 
       function w3_close() {
        document.getElementById("mySidebar").style.display = "none";
       }
    </script>
  </body>
</html>