 <body>
    <div class="jumbotron text-center" style="margin-bottom:0">
      <h1>CIRCUITATION </h1>
      <h4>A Circuit Designing And Simulation Website</h4> 
    </div>
    <div>
    <nav class="navbar navbar-expand-sm aa bg-dark navbar-dark ">
      <button class="w3-button text-white w3-xlarge" onclick="w3_open()">â˜°</button>
      <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link text-white" href="/page">BACK</a>
          </li>
          <li class="nav-item ">
            <a class="nav-link text-white" href="/logout">LOGOUT</a>
          </li>
        </ul>
      </nav>
    </div>

   <!-- Sidebar -->
   <div class="w3-sidebar w3-light-grey w3-bar-block w3-border-right" style="display:none" id="mySidebar">
    <button onclick="w3_close()" class="w3-bar-item w3-large">CLOSE &times;</button>
      <div class="list-group-item list-group-item-">
        <center><h2>USER'S INPUT</h2></center>
      </div>
      <form class="w3-container">
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Vin</label>
          <input type="text" id="Vin" class="form-control" value = "<%=c_Values.v_psv.v_in%> volt %>"  placeholder="Vin">
        </div>
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Frequency</label>
          <input type="text" id="Frequency" class="form-control" value = "<%=c_Values.v_psv.freq%> Hz"  placeholder="Frequency">
        </div>
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Vout From</label>
          <input type="text" id="Vout From" class="form-control" value = "<%=c_Values.v_psv.vout%> volt" placeholder="Vout From">
        </div>
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">VOut To</label>
          <input type="text" id="VOut To" class="form-control" value = "<%=c_Values.v_neg.vout%>"  placeholder="VOut To">
        </div>
      </form><br>

      <div class="list-group-item list-group-item-">
        <center><h2>OUTPUT</h2></center>
      </div><br>

     <form class="w3-container">
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Capacitor (C1)</label>
          <input type="text" id="Capacitor" class="form-control" value = "<%=c_Values.v_psv.c%>" placeholder="Capacitor">
        </div>
        <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Capacitor (C2)</label>
            <input type="text" id="Capacitor" class="form-control" value = "<%=c_Values.v_neg.c%>" placeholder="Capacitor">
          </div>
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Transformer</label>
          <input type="text" id="Transformer" class="form-control" value = "<%=c_Values.v_psv.v_a%> VA" placeholder="Transformer">
        </div>
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Resistor</label>
          <input type="text" id="Resistor" class="form-control"value = "<%=c_Values.v_psv.R2%> ohm"  placeholder="Resistor">
        </div>
        <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Resistor</label>
            <input type="text" id="Resistor" class="form-control"value = "<%=c_Values.v_neg.R2%> ohm"  placeholder="Resistor">
          </div>
        <fieldset disabled>
        <div class="form-group">
          <label for="disabledTextInput">Voltage Regulator</label>
          <input type="text" id="Voltage Regulator" class="form-control" value = "<%=c_Values.v_psv.img_label%>"  placeholder="Voltage Regulator">
        </div>
        <fieldset disabled>
          <div class="form-group">
            <label for="disabledTextInput">Voltage Regulator</label>
            <input type="text" id="Voltage Regulator" class="form-control" value = "<%=c_Values.v_neg.img_label%>"  placeholder="Voltage Regulator">
          </div>
     </form>
   </div>

    <!-- Page Content -->
    <div class="">

      <div class="w3-container">
        <h1></h1>
      </div>

      <div class="imagebox">
        <br><br>

